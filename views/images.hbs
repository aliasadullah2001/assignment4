<div class="row">
<div class="col-md-12">
  
    <h2>Images</h2>
    <hr/>

</div> 

{{#if data}}
    {{#each data}}
    <div class="col-md-4">  
    <img src= "/images/uploaded/{{this}}" class= "img-responsive img-thumbnail" />      
    </div>
    {{/each}}

{{else}}

    <div class="col-md-12 text-center">
        <strong>No Images avaliable</strong>
    </div>

{{/if}}

</div></div>